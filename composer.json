{
  "name": "ofthewildfire/filament-inline-edit-column",
  "description": "Click-to-edit columns for Filament with automatic custom fields integration",
  "keywords": [
    "filament",
    "laravel",
    "inline-edit",
    "table",
    "column"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "OfTheWildfire"
    }
  ],
  "require": {
    "php": "^8.1",
    "filament/filament": "^3.0",
    "illuminate/contracts": "^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "OfTheWildfire\\FilamentInlineEditColumn\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OfTheWildfire\\FilamentInlineEditColumn\\InlineEditPluginServiceProvider"
      ]
    }
  }
}
